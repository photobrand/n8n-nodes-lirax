{
  "name": "n8n-nodes-lirax",
  "version": "1.0.0",
  "description": "Official LiraX telephony and CRM integration for n8n with full API coverage - Voice calls, SMS, IVR, contacts and call management",
  "type": "module",
  "engines": {
    "node": ">=18.17.0",
    "n8n": ">=1.111.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && npm run lint",
    "watch": "tsc -w -p tsconfig.json",
    "lint": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build && npm run test",
    "dev": "npm run watch",
    "prepack": "npm run build"
  },
  "keywords": [
    "n8n-community-node-package",
    "n8n-node",
    "lirax",
    "telephony",
    "voip",
    "crm",
    "ivr",
    "pbx",
    "voice",
    "calls",
    "sms",
    "contacts",
    "integration",
    "ai-agent",
    "workflow-automation"
  ],
  "author": "PRAGMA <info@pragma.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pragma-company/n8n-nodes-lirax.git"
  },
  "bugs": {
    "url": "https://github.com/pragma-company/n8n-nodes-lirax/issues"
  },
  "homepage": "https://github.com/pragma-company/n8n-nodes-lirax#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/pragma-company"
  },
  "dependencies": {
    "qs": "^6.12.1",
    "luxon": "^3.4.4",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.23.0"
  },
  "optionalDependencies": {
    "ioredis": "^5.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/qs": "^6.9.8",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0",
    "n8n-workflow": "^1.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "extensionsToTreatAsEsm": [".ts"],
    "moduleNameMapping": {
      "^(\\.{1,2}/.*)\\.js$": "$1"
    },
    "transform": {
      "^.+\\.ts$": ["ts-jest", {
        "useESM": true,
        "tsconfig": "tsconfig.json"
      }]
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "lirax.svg"
  ],
  "n8n": {
    "credentials": [
      "dist/credentials/LiraXApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/LiraX/LiraX.node.js",
      "dist/nodes/LiraXTrigger/LiraXTrigger.node.js",
      "dist/nodes/LiraXTool/LiraXTool.node.js"
    ]
  }
}